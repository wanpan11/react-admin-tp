{
  "name": "react-admin-tp",
  "version": "1.0.0",
  "description": "react-ts 后台管理系统项目模版内置菜单路由、状态管理多环境打包等工具开箱即用",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "react",
    "admin",
    "typescript"
  ],
  "author": "wanpan11",
  "license": "ISC",
  "homepage": "https://wanpan11.github.io/react-admin-tp/",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config ./scripts/webpack.config.cjs --mode=development",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/webpack.config.cjs --mode=production",
    "build:homepage": "cross-env NODE_ENV=homepage webpack --config ./scripts/webpack.config.cjs --mode=production",
    "dev:build": "cross-env NODE_ENV=development webpack --config ./scripts/webpack.config.cjs --mode=development",
    "lint": "eslint --fix --ext js,jsx,ts,tsx src/ && tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.24",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.3.0",
    "mini-css-extract-plugin": "^2.8.1",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.36",
    "postcss-loader": "^7.3.4",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.9.5",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.4",
    "@babel/runtime": "^7.24.0",
    "@unocss/reset": "^0.53.6",
    "ahooks": "^3.7.10",
    "antd": "^5.15.3",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "core-js": "^3.36.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^3.4.3",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
